pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'sample step 01'
                echo 'sample step 02'
                echo 'sample step 03'
            }
            
        }
        stage('DevOps'){
            steps {
                echo 'Hello DevOps'
                echo 'sample step 02'
            }
        }
        stage('Hello Welcome') {
            steps {
                echo 'Hello World'
                echo 'sample step 02'
            }

        }


    }
}
